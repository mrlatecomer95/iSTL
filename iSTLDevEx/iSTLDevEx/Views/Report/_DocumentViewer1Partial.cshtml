@Html.DevExpress().DocumentViewer(settings => {
    settings.Name = "DocumentViewer1";
    settings.CallbackRouteValues = new { Controller = "Report", Action = "DocumentViewer1Partial" };
    settings.ExportRouteValues = new { Controller = "Report", Action = "DocumentViewer1PartialExport" };
	settings.Report = (XtraReport)Model;
	settings.ToolbarMode = DocumentViewerToolbarMode.StandardToolbar;
}).GetHtml()